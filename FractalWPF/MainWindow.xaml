<Window x:Class="FractalWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FractalWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button Content="Create project" HorizontalAlignment="Center" Margin="0,225,0,0" VerticalAlignment="Top" Width="175" Height="25" Click="create_project"/>
        <Button Content="Open project" HorizontalAlignment="Center" Margin="0,255,0,0" VerticalAlignment="Top" Width="175" Height="25" Click="open_project"/>
        <Button Content="Exit" HorizontalAlignment="Center" Margin="0,285,0,0" VerticalAlignment="Top" Width="175" Height="25" Click="exit"/>
        <Button Content="FAQ" HorizontalAlignment="Right" Margin="0,10,20,0" VerticalAlignment="Top" Width="75" Click="faq"/>
        <Label Content="Welcome to noSense" HorizontalAlignment="Center" Margin="0,100,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
